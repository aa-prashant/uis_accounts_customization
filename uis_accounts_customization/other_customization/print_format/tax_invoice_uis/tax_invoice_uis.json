 "font_size": 14,
 "html": " <!-- Bootstrap CSS \n    <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.rtl.min.css\" integrity=\"sha384-DOXMLfHhQkvFFp+rWTZwVlPVqdIhpDVYT9csOnHSgWQWPX0v5MCGtjCJbY6ERspU\" crossorigin=\"anonymous\">\n-->\n<div class=\"row\">\n    <div class=\"col-sm-2 col-xs-2 \">\n            <img src=\"https://uis-erp.com/private/files/uiscompany_logo.jpeg\"  height=\"80\" width=\"100\"/>\n        </div>\n        \n        <div class=\"col-sm-7 col-xs-7\" >\n          <strong style=\"font-size: 12px;\">  {{_(\"United Industrial Inspection Services Co. (UIS-INSPECCO)\")}} </strong><br>\n          <strong style=\"font-size: 12px;\">  {{_(\"\u0634\u0631\u0643\u0629 \u0627\u0644\u0645\u062a\u062d\u062f\u0629 \u0644\u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0641\u062d\u0635 \u0627\u0644\u0635\u0646\u0627\u0639\u064a ( \u064a\u0648 \u0622\u064a \u0627\u0633 - \u0627\u0646\u0633\u0628\u064a\u0643\u0648)\")}} </strong><br>\n\n            <strong style=\"font-size: 8px;\">{{_(\"Address : Al-Otaishan TowerBuilding, GCC Route 95Al Manar District\")}}</strong> <br>\n           <strong style=\"font-size: 8px;\"> {{_(\"Mezzanine Floor, P.O Box 6266, Dammam 31442, Kingdom of Saudi Arabia\")}} </strong><br>\n           <strong style=\"font-size: 8px;\"> {{_(\"-Phone : +966920010472  -Fax : +966138350666\")}}</strong> <br>\n            <strong style=\"font-size: 8px;\">{{_(\"Email : info@uis-services.com - Website : www.uis-services.com\")}} </strong><br>\n            <strong style=\"font-size: 8px;\">{{_(\"VAT No. : 310185115800003 - Commercial Reg. : 2050095733\")}} </strong>\n            \n        </div>\n        <div class=\"col-sm-2 col-xs-2 \">\n            <img src=\"{{doc.ksa_einv_qr}}\"  height=\"80\" width=\"100\"/>\n        </div>\n        \n    </div>\n    <hr>\n    \n    <div class=\"text-center\" style=\"font-size: 10px;\"> <b>Tax Invoice</b></div>\n\n<table class=\"table table-bordered\">\n    <tr>\n      <td style=\"font-size: 8px; width:50%\">Invoice No. : {{doc.name}}</td>\n      <td style=\"font-size: 8px; width:50%\">Customer Name : {{doc.customer}}</td>\n    </tr>\n    <tr>\n      <td style=\"font-size: 8px; width:50%\">Date : {{doc.posting_date}}</td>\n      <td style=\"font-size: 8px; width:50%\">Contact Person : {{doc.contact_display}}</td>\n    </tr>\n       <tr>\n      <td style=\"font-size: 8px; width:50%\">Created By : {{doc.custom_created_by}} </td>\n      <td style=\"font-size: 8px; width:50%\">VAT NO. : {{doc.tax_id}} </td>\n    </tr>\n       <tr>\n      <td style=\"font-size: 8px; width:30%\">Payment Terms : {{doc.payment_terms_template}}</td>\n      <td style=\"font-size: 8px; width:70%\">Address : {{doc.customer_address}} </td>\n    </tr>\n       <tr>\n      <td style=\"font-size: 8px; width:50%\">Due Date : {{doc.due_date}}</td>\n      <td style=\"font-size: 8px; width:50%\">Phone : {{doc.contact_phone}}- Mobile : {{doc.contact_mobile}}  - Fax : {{doc.contact_mobile}}</td>\n   \n    </tr>\n    <tr>\n      <td style=\"font-size: 8px; width:50%\">Recieved Date : {{doc.posting_date}}</td>\n      <td style=\"font-size: 8px; width:50%\">PO NO. : {{doc.po_no}} - PO Date : {{doc.po_date}}</td>\n   \n    </tr>\n</table>\n\n<!-- items -->\n            <table class=\"table table-bordered\">\n                <tr style=\"background-color:#1a345a;\">\n                    <td style=\"font-size: 8px;\">No.</td>\n                    <td style=\"font-size: 8px;\">Service Name</td>\n                    <td style=\"font-size: 8px;\">UOM</td>\n                    <td style=\"font-size: 8px;\">Qty</td>\n                    <td style=\"font-size: 8px;\"> Price</td>\n                    <td style=\"font-size: 8px;\">Gross Amt</td>\n                \n                </tr>\n                {% for item in doc.items %}\n                <tr>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ loop.index }}</td>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ item.item_name }}</td>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ item.uom }}</td>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ item.qty}}</td>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ item.rate or 0}} {{doc.currency}}</td>\n                    <td style=\"text-align: center; font-size: 8px;\">{{ item.amount }} {{doc.currency}}</td>\n                    \n                </tr>\n                {% endfor %}\n                \n             </table>\n\n\n             \n",
